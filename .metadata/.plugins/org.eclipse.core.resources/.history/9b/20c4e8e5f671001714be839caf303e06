package zaaviyah;

public class MethodsAndClasses {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		///////////////Overloading Methods//////////////////
		double vol ;
		Box obj = new Box(10, 20, 15);
		///get volume of first box
		vol = obj.volume();
		System.out.println("Volume of mybox1 is " + vol);
	}

}


class Box {
	
//		double width;
//		double height;
//		double depth;
//		// This is the constructor for Box.
//		Box(double w, double h, double d) {
//		width = w;
//		height = h;
//		depth = d;
//		}
//		// compute and return volume
//		double volume() {
//		return width * height * depth;
//		}
		
		double width ;
		double height ;
		double depth ;
		// this is constructor for box
		
		Box (double w, double h , double d) {
			width =w;
			height = h;
			depth = d;
		} 
		
		/// compute and return
		double volume () {
			return width * height * depth ;
		}
		
}