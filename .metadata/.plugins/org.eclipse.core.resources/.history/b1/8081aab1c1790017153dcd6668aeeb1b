package zaaviyahSS;

public class Home {

	
	  public int age = 10 ;
	  
	  public void setAge (int newAge) {
		  age = newAge ;
	  }
	  
	  public  int ageGet () {
		   return age ;
	   }
	   
	

}

  class Room extends  Home {
	 

	public static void main(String[] args) {
		// TODO Auto-generated method stub	
	
		Room obj = new Room ();
		obj.setAge (2); 
		System.out.println("shah" + obj.ageGet() ); 	
		
		
		

	}
	
}