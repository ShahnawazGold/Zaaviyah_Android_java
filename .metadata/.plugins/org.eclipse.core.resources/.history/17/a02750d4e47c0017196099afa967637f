public class methods {

	/// local method
	public void  ageSet () {
		
		int age = 23;
		System.out.println("age is" +age);
		
	}
	
	/// instance method
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		methods obj = new methods ();
		obj.ageSet();
		
		
	}

}
