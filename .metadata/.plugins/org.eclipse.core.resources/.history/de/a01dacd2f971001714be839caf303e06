//Objects may be passed to methods.
class Test {
//int a, b;
//	Test(int i, int j) {
//	a = i;
//	b = j;
//	}
//	//return true if o is equal to the invoking object
//	boolean equalTo(Test o) {
//	if(o.a == a && o.b == b) return true;
//	else return false;
//	}
//	
	int a , b;
	 Test (int i , int j) {
		 a = i;
		 b = j;
		 
	 }
	 
	 ///return  true if  o is equal to invoking object 
	 
	 boolean equalTo ( Test o) {
		  if (o.a==a && o.b== b) return  true ;
		  else return false ;
	 }
}