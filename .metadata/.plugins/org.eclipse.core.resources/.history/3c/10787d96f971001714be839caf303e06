///////////////// xmple 1 ///////////////////
class Box {
	
		
		double width ;
		double height ;
		double depth ;
		// this is constructor for box
		
		Box (double w, double h , double d) {
			width =w;
			height = h;
			depth = d;
		} 
		
		/// compute and return
		double volume () {
			return width * height * depth ;
		}
		
}